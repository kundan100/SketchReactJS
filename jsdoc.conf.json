{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./src", "./public"],
        "exclude": ["./node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": ""
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./jsdoc_out/",
        "recurse": true        
    }
}